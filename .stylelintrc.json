{
    "extends": "stylelint-config-standard",
    "rules": {
        "indentation": 4,
        "string-quotes": "double",
        "rule-empty-line-before": "never",
        "at-rule-empty-line-before": "never",
        "at-rule-no-unknown": [
            true,
            {
                "ignoreAtRules": [
                    "mixin",
                    "include"
                ]
            }
        ],
        "font-family-no-missing-generic-family-keyword": null,
        "declaration-empty-line-before": "never"
    }
}